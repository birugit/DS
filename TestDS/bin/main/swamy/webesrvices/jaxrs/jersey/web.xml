<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xml="http://www.w3.org/XML/1998/namespace" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.w3.org/2001/XMLSchema http://www.w3.org/2001/XMLSchema " id="WebApp_ID" version="3.0">
<servlet>
	<servlet-name>Jersy Rest Service</servlet-name>
	<servlet-class>org.glassfish.jersey.servlet.ServletContainer</servlet-class>
	<init-param>
		<param-name>jersey.config.server.provider.packages</param-name>
		<param-value>main.swamy.webesrvices.jaxrs.jersey</param-value>
	</init-param>
	<load-on-startup>1</load-on-startup>
</servlet>
<servlet-mapping>
	<servlet-name>Jersy Rest Service</servlet-name>
	<url-pattern>/rest/*</url-pattern>
</servlet-mapping>

</web-app>
